{
    "compilerOptions": {
        "declaration": false,
        "jsx": "react",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noLib": false,
        "removeComments": true,
        "sourceMap": true,
        "module": "commonjs",
        "lib": ["es5", "es6", "dom"],
        "allowJs": true,
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "target": "es6",
        "strictNullChecks": true
    }
}